{
    "manifest_version": 3,
    "id": "heavy-math-solver",
    "inference": {
        "default_mode": "local",
        "local_entry": "backend.py"
    },
    "content_scripts": [
        {
            "matches": [
                "*://ticket.melon.com/*",
                "*://*.interpark.com/*"
            ],
            "js": [
                "thisscript.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}