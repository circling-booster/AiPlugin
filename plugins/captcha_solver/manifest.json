{
    "manifest_version": 3,
    "id": "captcha_solver",
    "name": "Universal Captcha Solver",
    "requirements": {},
    "inference": {
        "execution_type": "none",
        "supported_modes": [
            "web"
        ],
        "default_mode": "web",
        "local_entry": "backend.py",
        "models": [
            {
                "key": "MODEL_MELON",
                "filename": "model_melon.pt"
            }
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "*://ticket.melon.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ]
}