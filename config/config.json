{
  "system_settings": {
    "proxy_port": "auto",
    "api_port": "auto",
    "ssl_passthrough": [
      "*.bank.co.kr",
      "*.gov.kr",
      "localhost",
      "127.0.0.1"
    ],
    "trusted_types_bypass": [
      "*youtube.com",
      "*youtu.be",
      "*google.com"
    ],
    "cloud_inference": {
      "base_url": "http://localhost:8000",
      "system_api_key": "sk-system-secure-key-12345"
    },
    "security_policy": {
      "apply_to": [
        "*"
      ],
      "bypass_cors": false,
      "bypass_csp": true,
      "bypass_frame_options": false,
      "allow_autoplay": true,
      "allow_insecure_cert": false,
      "auto_grant_permissions": false
    }
  },
  "user_settings": {
    "theme": "dark",
    "auto_start": false
  }
}